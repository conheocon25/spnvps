Options +FollowSymlinks
RewriteEngine on

# compress text, html, javascript, css, xml:
AddOutputFilterByType DEFLATE text/plain
AddOutputFilterByType DEFLATE text/html
AddOutputFilterByType DEFLATE text/xml
AddOutputFilterByType DEFLATE text/css
AddOutputFilterByType DEFLATE application/xml
AddOutputFilterByType DEFLATE application/xhtml+xml
AddOutputFilterByType DEFLATE application/rss+xml
AddOutputFilterByType DEFLATE application/javascript
AddOutputFilterByType DEFLATE application/x-javascript

#---------------------------------------------------------------------------------------------------------------------------
# SYSTEM
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^signin/load$ 			?cmd=SigninLoad [L]
RewriteRule ^signin/exe$ 			?cmd=SigninExe [L]
RewriteRule ^signout/load$ 			?cmd=SignoutLoad [L]
RewriteRule ^signout/exe$ 			?cmd=SignoutExe [L]

RewriteRule ^object/load/([A-Za-z0-9]+)/([0-9]+)$ 					?cmd=ObjectLoad&ObjectName=$1&Id=$2 [L]
RewriteRule ^object/del/([A-Za-z0-9]+)/([0-9]+)$ 					?cmd=ObjectDel&ObjectName=$1&Id=$2 [L]
RewriteRule ^object/ins/([A-Za-z0-9]+)$ 							?cmd=ObjectIns&ObjectName=$1 [L]
RewriteRule ^object/upd/([A-Za-z0-9]+)$ 							?cmd=ObjectUpd&ObjectName=$1 [L]

#---------------------------------------------------------------------------------------------------------------------------
# SELLING
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^app$ 												?cmd=App [L]

RewriteRule ^app/config$ 										?cmd=AppConfig [L]
RewriteRule ^app/config/page/([0-9]+)$ 							?cmd=AppConfig&Page=$1 [L]

RewriteRule ^app/province$ 										?cmd=AppProvince [L]
RewriteRule ^app/province/([0-9]+)$ 							?cmd=AppDistrict&IdProvince=$1 [L]
RewriteRule ^app/province/([0-9]+)/([0-9]+)$ 					?cmd=AppPagoda&IdProvince=$1&IdDistrict=$2 [L]

RewriteRule ^app/province/([0-9]+)/([0-9]+)/([0-9]+)/monk$ 		?cmd=AppPagodaMonk&IdProvince=$1&IdDistrict=$2&IdPagoda=$3 [L]
RewriteRule ^app/province/([0-9]+)/([0-9]+)/([0-9]+)/event$ 	?cmd=AppPagodaEvent&IdProvince=$1&IdDistrict=$2&IdPagoda=$3 [L]
RewriteRule ^app/province/([0-9]+)/([0-9]+)/([0-9]+)/video$ 	?cmd=AppPagodaVideo&IdProvince=$1&IdDistrict=$2&IdPagoda=$3 [L]
RewriteRule ^app/province/([0-9]+)/([0-9]+)/([0-9]+)/post$ 		?cmd=AppPagodaPost&IdProvince=$1&IdDistrict=$2&IdPagoda=$3 [L]
RewriteRule ^app/province/([0-9]+)/([0-9]+)/([0-9]+)/album$ 	?cmd=AppPagodaAlbum&IdProvince=$1&IdDistrict=$2&IdPagoda=$3 [L]
